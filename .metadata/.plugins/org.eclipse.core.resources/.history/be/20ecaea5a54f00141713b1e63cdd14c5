	public void setDefaultColorList(LinkedList<Color> l){
		addColor(Color.GREEN, l);
		addColor(Color.RED, l);
		addColor(Color.YELLOW, l);
		addColor(Color.BLUE, l);
		addColor(Color.MAGENTA, l);

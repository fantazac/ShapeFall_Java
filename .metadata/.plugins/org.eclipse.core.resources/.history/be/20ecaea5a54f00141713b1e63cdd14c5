			goo = gameOverOptionMax;
		}
		if(goo < 0){
			goo = 0;
		}
		gameOverOption = goo;
	}
	
	//sets the initial musicOption
	public void setInitialMusicOption(){
		if(mainMusic.equals("pokemon.wav")){
			musicOption = 0;
		}
		else if(mainMusic.equals("ghostbusters.wav")){
			musicOption = 1;
		}
		else if(mainMusic.equals("trololol.wav")){
			musicOption = 2;
		}
	}
	
